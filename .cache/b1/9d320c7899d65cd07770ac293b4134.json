:double;\r\n    width: 50%;\r\n     margin: 100px auto;\r\n     box-sizing: border-box;\r\n }\r\n\r\n.presentacion {\r\n    background: #3D3D3D;\r\n    padding: 20px;\r\n    -webkit-border-radius: 10px;\r\n    -moz-border-radius: 10px;\r\n    border-radius: 10px;\r\n    color:rgb(52, 168, 17);\r\n    border-color: black;\r\n    border-width: thick;\r\n    border-style:double;\r\n    width: 50%;\r\n     margin: 100px auto;\r\n     box-sizing: border-box;\r\n\r\n}\r\n\r\n@media only screen and (max-width: 600px) {\r\n    .portadadiv {\r\n        width: 100%;\r\n    }\r\n    .portadadiv .hider {\r\n        display: none;\r\n    }\r\n}\r\n\r\n\r\n\r\n footer{\r\n     position:relative; \r\n    width:98%;\r\n    height:5%;\r\n    color:rgb(0, 0, 0);\r\n    clear:both;\r\n     margin-bottom: 20px;\r\n    \r\n }\r\n footer div {\r\n    background-color:rgb(195, 216, 125);\r\n    text-align: center;\r\n    border: 1px solid darkgreen;\r\n    border-radius: 10px;\r\n    max-width: 320px;\r\n    color:rgb(0, 0, 0);\r\n     display: inline-block;\r\n     padding: 10px 15px;\r\n }\r\n footer div a{\r\n    text-align: center;\r\n    color:rgb(31, 145, 238)\r\n}\r\n\r\n footer .copyright {\r\n     float: right;\r\n     color: #fff;\r\n }\r\nheader,section,footer,aside,article,nav{\r\nborder-radius: 10px;}\r\n\r\n\r\n/*****Presentacion*****/\r\n.lista {\r\n    width: 300px;\r\n    padding: 20px;\r\n    box-sizing: border-box;\r\n    color: black;\r\n    border:5px solid black;\r\n    border-radius: 10px;\r\n    background-color: rgba(245, 255, 250, 0.747);\r\n}\r\n\r\n.lista h3 {\r\n    margin-bottom: 20px;\r\n}\r\n\r\n.lista-links {\r\n    list-style: none;\r\n}\r\n\r\n.lista-links li {\r\n    text-decoration: underline;\r\n    cursor: pointer;\r\n    margin-bottom: 10px;\r\n}\r\n\r\n.lista-links li:hover {\r\n    color: #000;\r\n}\r\n\r\n.content {\r\n    width: calc(100% - 300px);\r\n    background: #3D3D3D;\r\n    padding: 20px;\r\n    box-sizing: border-box;\r\n    display: flex;\r\n}\r\n\r\n.content img {\r\n    margin: 0 20px 10px 0;\r\n    display: block;\r\n}\r\n\r\n.content .tab-content {\r\n    display: flex;\r\n}\r\n\r\n.content .tab-content.show {\r\n    display: flex;\r\n}\r\n.linksdiv {\r\n    background-color:rgba(199, 236, 198, 0.87);\r\n    padding: 20px;\r\n    -webkit-border-radius: 10px;\r\n    -moz-border-radius: 10px;\r\n    border-radius: 10px;\r\n    color:black;\r\n    border-color: black;\r\n    border-width: thick;\r\n    border-style:double;\r\n    width: 50%;\r\n     margin: 100px auto;\r\n     box-sizing: border-box;\r\n}"},"lineCount":null}},"error":null,"hash":"b69c7d8570b2322ff134ad98917d6afb","cacheData":{"env":{}}}